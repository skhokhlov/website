module "error"

match / {
html('<!DOCTYPE html>')
<html>
<head>
    <!--<meta name="viewport" content="width=device-width, initial-scale=1"/>-->
    <title>{ .code } { .title }</title>
    <link rel="stylesheet" href="/public/app.css" />
    <link type="image/x-icon" rel="shortcut icon" href="/images/icon/icon.ico"/>
</head>
